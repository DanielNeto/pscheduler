#!/bin/sh -e
#
# Clean all of the pScheduler source directories
#

WHEREAMI=$(dirname "$0")

# Cleaning requires that make-generic-package be installed.
if [ ! -e /usr/include/make/generic-package.make ]
then
    make -C "${WHEREAMI}/../make-generic-package" clean build install
fi


for PACKAGE in $("${WHEREAMI}/build-order")
do
    make -C "${PACKAGE}" clean
done
