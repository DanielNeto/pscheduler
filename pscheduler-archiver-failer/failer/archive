#!/usr/bin/python
#
# Fail at archiving most of the time.
#

import pscheduler
import random
import sys

random.seed()

if random.random() > 0.8:
    json = {
        "succeeded": True
        }
else:
    json = {
        "succeeded": False,
        "error": "Induced random failure",
        "retry": "PT10S"
        }

pscheduler.succeed_json(json)
